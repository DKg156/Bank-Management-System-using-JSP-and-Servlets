<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>Check</servlet-name>
        <servlet-class>Check</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>New</servlet-name>
        <servlet-class>New</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CheckAdmin</servlet-name>
        <servlet-class>CheckAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewName</servlet-name>
        <servlet-class>NewName</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewPass</servlet-name>
        <servlet-class>NewPass</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>NewType</servlet-name>
        <servlet-class>NewType</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Delete</servlet-name>
        <servlet-class>Delete</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Check</servlet-name>
        <url-pattern>/Check</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>New</servlet-name>
        <url-pattern>/New</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CheckAdmin</servlet-name>
        <url-pattern>/CheckAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewName</servlet-name>
        <url-pattern>/NewName</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewPass</servlet-name>
        <url-pattern>/NewPass</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>NewType</servlet-name>
        <url-pattern>/NewType</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Delete</servlet-name>
        <url-pattern>/Delete</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
